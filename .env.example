# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration (if needed for external services)
EXPO_PUBLIC_API_BASE_URL=https://api.virtualroom.app
EXPO_PUBLIC_AI_SERVICE_URL=https://ai.virtualroom.app

# App Configuration
EXPO_PUBLIC_APP_NAME=Virtual Room
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENVIRONMENT=development

# Analytics (optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Feature Flags
EXPO_PUBLIC_ENABLE_PREMIUM_FEATURES=true
EXPO_PUBLIC_ENABLE_SOCIAL_SHARING=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=false